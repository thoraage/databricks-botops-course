   "outputs": [],
   "source": [
    "cat = catname_from_path()\n",
    "db = dbname(db=\"taxinycbots\", cat=cat)\n",
    "print(\"New db name: \" + db)\n",
    "spark.sql(f\"USE catalog {cat}\")\n",
    "spark.sql(f\"CREATE DATABASE IF NOT EXISTS {db}\")"
   ]
